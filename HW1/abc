#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>

int main(int argc, char *argv[])
{
    int fd = open("abc", O_RDONLY);
    int fd2 = fd;
    close(fd);
    int n;
    char buf[1024];
    while ((n = read(fd2, buf, 1024)) > 0) {
        write(STDOUT_FILENO, buf, n);
    }
    return 0;
}